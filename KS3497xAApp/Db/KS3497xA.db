record(mbbo, "$(P)$(R)TrigSrc") {
    field(DESC, "Trigger source")
    #field(DTYP, "stream")
    #field(OUT,  "@Keysight3497xA.proto setTrigSource $(PORT)")
    field(ZRVL, "0")
    field(ZRST, "BUS")
    field(ONVL, "1")
    field(ONST, "IMM")
    field(TWVL, "2")
    field(TWST, "EXT")
    field(THVL, "3")
    field(THST, "ALAR1")
    field(FRVL, "4")
    field(FRST, "ALAR2")
    field(FVVL, "5")
    field(FVST, "ALAR3")
    field(SXVL, "6")
    field(SXST, "ALAR4")
    field(SVVL, "7")
    field(SVST, "TIM")
    field(FLNK, "$(P)$(R)TrigSrc_RBV")
}

record(mbbo, "$(P)$(R)Card1MonSelect01_16") {
    field(DESC, "Card 1 monitor 1-16 selection")
