record(stringin,"$(P)$(R)Model_RBV") {
    field(DESC, "ID string")
    field(SCAN, "I/O Intr")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(PORT),$(ADDR=0))Model")
}

